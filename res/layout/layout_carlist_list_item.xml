<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_carlist_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2F2F2F"
    android:padding="10dp" >

	<ImageView
        android:id="@+id/layout_carlist_list_img_Lock"
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:layout_alignBottom="@+id/layout_carlist_txt_name"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="10dp"
        android:src="@drawable/layout_carlist_list_img_lock" />

    <TextView
        android:id="@+id/layout_carlist_txt_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:textStyle="bold"
	    android:typeface="sans" />

</RelativeLayout>